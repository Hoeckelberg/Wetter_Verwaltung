<Window x:Class="WetterVerwaltung.ScottPlotView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WetterVerwaltung"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="ScottPlotView" Height="450" Width="800">
    <Grid>
        <WpfPlot x:Name="Plot1" Margin="0,16,0,0" HorizontalAlignment="Center" Width="756" Height="380" VerticalAlignment="Top">
        </WpfPlot>
        <Button x:Name="BTN_Save" Click="BTN_Save_Click" Content="Save as PNG" Margin="0,396,0,0" HorizontalAlignment="Center" Width="70" Height="33" VerticalAlignment="Top"/>
    </Grid>
</Window>
